<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!--组里面可以定义组;定义测试的数据可以从xml文件的parameter里来-->
<suite name="TestWebServer">
    <test name="T1">
        <parameter name="number-of-times" value="10"/>
        <classes>
            <class name="TestPractise.TestWebServer"/>
        </classes>
    </test>
    <test name="T2">
        <parameter name="number-of-times" value="15"/>
        <classes>
            <class name="TestPractise.TestWebServer"/>
        </classes>
    </test>
    <test name="T3">
        <parameter name="number-of-times" value="20"/>
        <classes>
            <class name="TestPractise.TestWebServer"/>
        </classes>
    </test>
    <test name="T4">
        <parameter name="number-of-times" value="25"/>
        <classes>
            <class name="TestPractise.TestWebServer"/>
        </classes>
    </test>
</suite>